{
  "name": "@ocelescope/api-client",
  "type": "module",
  "version": "0.0.0",
  "description": "Core client + session/hooks layer used by Ocelescope API packages.",
  "author": "Görkem-Emre Öztürk <goerkem.oeztuerk@rwth-aachen.de>",
  "license": "MIT",
  "homepage": "https://www.ocelescope.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/promi4s/ocelescope.git"
  },
  "bugs": {
    "url": "https://github.com/promi4s/ocelescope/issues"
  },
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^2.0.8",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "bumpp": "^10.3.2",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3"
  }
}
